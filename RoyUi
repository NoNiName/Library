local a,b=game:GetService'TweenService',{}b.__index=b local c={duration=0.2,style=Enum.EasingStyle.Quad,direction=Enum.EasingDirection.Out}function b.new(d)local e=setmetatable({instance=d,_tweens={},_connections={}},b)return e end function b._mergeConfig(d,e)e=e or{}return{duration=e.duration or c.duration,style=e.style or c.style,direction=e.direction or c.direction}end function b._createTween(d,e,f)local g=d:_mergeConfig(f)if d._tweens[d.instance]then d._tweens[d.instance]:Cancel()end local h=a:Create(d.instance,TweenInfo.new(g.duration,g.style,g.direction),e)d._tweens[d.instance]=h h:Play()return h end function b.Fade(d,e,f)return d:_createTween({BackgroundTransparency=e,TextTransparency=e},f)end function b.Color(d,e,f)return d:_createTween({BackgroundColor3=e},f)end function b.Move(d,e,f)return d:_createTween({Position=e},f)end function b.Resize(d,e,f)return d:_createTween({Size=e},f)end function b.Hover(d,e,f,g)if d._connections.hover then d._connections.hover.enter:Disconnect()d._connections.hover.leave:Disconnect()end local h,i=d.instance.MouseEnter:Connect(function()d:_createTween(e,g)end),d.instance.MouseLeave:Connect(function()d:_createTween(f,g)end)d._connections.hover={enter=h,leave=i}end function b.Click(d,e,f,g)if d._connections.click then d._connections.click.press:Disconnect()d._connections.click.release:Disconnect()end local h,i=d.instance.MouseButton1Down:Connect(function()d:_createTween(e,g)end),d.instance.MouseButton1Up:Connect(function()d:_createTween(f,g)end)d._connections.click={press=h,release=i}end function b.Destroy(d)for e,f in pairs(d._tweens)do f:Cancel()end for g,h in pairs(d._connections)do if typeof(h)=='table'then for i,j in pairs(h)do j:Disconnect()end else h:Disconnect()end end d._tweens={}d._connections={}end local d={Light={Primary=Color3.fromHex'414141',Secondary=Color3.fromHex'FFFFFF',Ground=Color3.fromHex'F8F8F8',Last=Color3.fromHex'EEEEEE',Text=Color3.fromHex'575757'},Dark={Primary=Color3.fromHex'D9D9D9',Secondary=Color3.fro
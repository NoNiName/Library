.Visible=true H.main.current_page=L end end end)end function E.Select(H)local K=ad:Create(H.current.value,TweenInfo.new(ae.duration,ae.style),{BackgroundTransparency=0})K:Play()end function E.Deselect(H)local K=ad:Create(H.current.value,TweenInfo.new(ae.duration,ae.style),{BackgroundTransparency=1})K:Play()end function E.CreateSection(H,K:string)H.section_counter+=1 local L=ac.new{id=H.section_counter,name=K,main=H.main,page=H.current_page}H.sections[K]=L return L end local H,K=game:GetService'UserInputService',{}K.__index=K function K.new(L)local M,N=setmetatable({theme=d.Init(L and L.theme or'Dark'),tabs={},tabcount=0,current_tab=nil,pages={},pagecount=0,current_page=nil,instance={},dragging=false,dragStart=nil,startPos=nil,font_size={header=20,body=12},visible=true,toggleKey=L.toggleKey or'RightControl'},K),game:GetService'Players'.LocalPlayer.PlayerGui:FindFirstChild'RoyxGui'if N then N:Destroy()end local O=f.Instance('ScreenGui',{Name='RoyxGui',Parent=game:GetService'Players'.LocalPlayer.PlayerGui})local P=f.Instance('Frame',{Name='Main',Size=UDim2.fromOffset(700,600),BackgroundColor3=M.theme.Colors.Ground,Parent=O.value,Position=UDim2.fromScale(0.1,0),AnchorPoint=Vector2.new(0,0)})P:Rounded(16)P:Padding(12)P:Layout{FillDirection=Enum.FillDirection.Horizontal,Padding=UDim.new(0,12)}local Q=f.Instance('Frame',{Name='Infomation',Size=UDim2.fromScale(0.4,0),AutomaticSize=Enum.AutomaticSize.Y,BackgroundTransparency=1,Parent=P.value})Q:Padding(16)Q:Layout{FillDirection=Enum.FillDirection.Vertical,Padding=UDim.new(0,16)}local R=f.createText{Text='Royx Hub',TextColor3=M.theme.Colors.Text,TextSize=M.font_size.header,Font=Enum.Font.GothamBold,Parent=Q.value}R.value.Active=true local S=b.new(P.value)local T=function(T)if M.dragging then local U=T.Position-M.dragStart local V=UDim2.new(M.startPos.X.Scale,M.startPos.X.Offset+U.X,M.startPos.Y.Scale,M.startPos.Y.Offset+U.Y)S:_createTween({Position=V},{duration=0.1,style=Enum.EasingStyle.Linear})end end R.value.InputBegan:Connect(function(U)if U.UserInputType==Enum.UserInputType.MouseButton1 then M.dragging=true M.dragStart=U.Position M.startPos=P.value.Position end end)game:GetService'UserInputService'.InputEnded:Connect(function(U)if U.UserInputType==Enum.UserInputType.MouseButton1 then M.dragging=false end end)game:GetService'UserInputService'.InputChanged:Connect(function(U)if U.UserInputType==Enum.UserInputType.MouseMovement then T(U)end end)local U=f.Instance('Frame',{Name='MainPage',Size=UDim2.fromScale(1,1),BackgroundColor3=M.theme.Colors.Secondary,Parent=P.value,ClipsDescendants=true})U:Rounded(12)U:Padding(20)U:Flex{FlexMode=Enum.UIFlexMode.Fill}local V=f.Instance('ScrollingFrame',{Name='Navigation',Size=UDim2.fromScale(1,1),BackgroundTransparency=1,ScrollBarThickness=0,AutomaticCanvasSize=Enum.AutomaticSize.Y,CanvasSize=UDim2.fromScale(0,0),ScrollingDirection=Enum.ScrollingDirection.Y,Parent=Q.value})V:Flex{FlexMode=Enum.UIFlexMode.Fill}V:Layout{FillDirection=Enum.FillDirection.Vertical,Padding=UDim.new(0,4)}H.InputBegan:Connect(function(W)if W.KeyCode.Name==M.toggleKey then M:Toggle()end end)M.instance={Navbar=V,Pages=U,Main=P,Animation=S,Title=R}return M end function K.CreateTab(L,M:string,N:string)L.tabcount+=1 local O={id=L.tabcount,name=M,icon=N,main=L}local P=E.new(O)L.tabs[P.id]=P return P end function K.UpdateTheme(L,M)L.theme=d.Init(M)local N=L.theme.Colors L.instance.Main.value.BackgroundColor3=N.Ground L.instance.Pages.value.BackgroundColor3=N.Secondary L.instance.Title.value.TextColor3=N.Text for O,P in pairs(L.tabs)do local Q=P.current Q.value.BackgroundColor3=N.Last Q.value:FindFirstChildOfClass('ImageLabel',true).ImageColor3=N.Primary Q.value:FindFirstChildOfClass('TextLabel',true).TextColor3=N.Primary for R,S in pairs(P.sections)do S:UpdateTheme()end end end function K.Toggle(L)L.visible=not L.visible L.instance.Main.value.Visible=L.visible end function K.SetToggleKey(L,M)assert(typeof(M)=='string','Toggle key must be a string')L.toggleKey=M end function K.IsVisible(L)return L.visible end local L={}function L.CreateWindow(M)local N,O=pcall(K.new,M)if not N then error(O)end return O end return L

and A.elements.searchbox then E=A.elements.searchbox.value.Text:lower()end A.filteredItems={}for F,G in ipairs(A.items)do if E==''or string.find(string.lower(G),E)then table.insert(A.filteredItems,G)end end for H,I in ipairs(A.filteredItems)do local J=A.multi and A.selectedItems[I]or I==A.selected local K=f.Instance('TextButton',{Size=UDim2.fromScale(1,0),AutomaticSize=Enum.AutomaticSize.Y,BackgroundColor3=J and A.main.theme.Colors.Primary or A.main.theme.Colors.Ground,BackgroundTransparency=J and 0 or 1,Text='',Parent=B.value})K:Padding(8)K:Rounded(6)local L=f.createText{Text=I,TextColor3=J and A.main.theme.Colors.Ground or A.main.theme.Colors.Text,TextSize=14,Parent=K.value}K.value.MouseButton1Click:Connect(function()A:SelectItem(I)end)if not J then local M=b.new(K.value)M:Hover({BackgroundTransparency=0.9},{BackgroundTransparency=1})end end end function z.SelectItem(A,B)if A.multi then A.selectedItems[B]=not A.selectedItems[B]if A.selectedItems[B]==false then A.selectedItems[B]=nil end A.elements.selected.value.Text=A:GetSelectedText()A:UpdateItems(A.elements.items)if A.callback then local D={}for E,H in pairs(A.selectedItems)do table.insert(D,E)end A.callback(D)end else A.selected=B A.elements.selected.value.Text=B A:ForceClose()A:UpdateItems(A.elements.items)if A.callback then A.callback(B)end end end function z.GetSelectedText(A)if A.multi then local B=0 for D in pairs(A.selectedItems)do B=B+1 end return B==0 and'None'or B..' selected'else return A.selected or'Select'end end function z.UpdateSearch(A,B)if B==''then A.filteredItems=A.items else A.filteredItems={}for D,E in ipairs(A.items)do if string.find(string.lower(E),string.lower(B))then table.insert(A.filteredItems,E)end end end A:UpdateItems(A.elements.items)end function z.Clear(A)if A.multi then A.selectedItems={}A.elements.selected.value.Text='None'if A.searchable and A.elements.searchbox then A.elements.searchbox.value.Text=''A.filteredItems=A.items end A:UpdateItems(A.elements.items)if A.callback then A.callback{}end else A.selected=nil A.elements.selected.value.Text='None'if A.searchable and A.elements.searchbox then A.elements.searchbox.value.Text=''A.filteredItems=A.items end A:UpdateItems(A.elements.items)if A.callback then A.callback(nil)end end end function z.AddItem(A,B)for D,E in ipairs(A.items)do if E==B then return false end end table.insert(A.items,B)if A.searchable then local H=A.elements.searchbox.value.Text if H==''or string.find(string.lower(B),string.lower(H))then table.insert(A.filteredItems,B)end else table.insert(A.filteredItems,B)end if A.opened then A:UpdateItems(A.elements.items)end return true end function z.RemoveItem(A,B)if B==nil then A.items={}A.filteredItems={}if A.multi then A.selectedItems={}else A.selected=nil end A.elements.selected.value.Text='None'if A.searchable and A.elements.searchbox then A.elements.searchbox.value.Text=''end elseif type(B)=='string'then local D=table.find(A.items,B)if D then table.remove(A.items,D)end D=table.find(A.filteredItems,B)if D then table.remove(A.filteredItems,D)end if A.multi then A.selectedItems[B]=nil elseif A.selected==B then A.selected=nil A.elements.selected.value.Text='None'end elseif type(B)=='table'then local D={}for E,H in ipairs(B)do D[H]=true end local I=#A.items while I>0 do if D[A.items[I] ]then table.remove(A.items,I)end I=I-1 end I=#A.filteredItems while I>0 do if D[A.filteredItems[I] ]then table.remove(A.filteredItems,I)end I=I-1 end if A.multi then for J,K in pairs(D)do A.selectedItems[J]=nil end elseif D[A.selected]then A.selected=nil A.elements.selected.value.Text='None'end end if A.multi then local D=0 for E in pairs(A.selectedItems)do D=D+1 end A.elements.selected.value.Text=D==0 and'None'or D..' selected'end if A.opened then A:UpdateItems(A.elements.items)end if A.callback then if A.multi then local D={}for E,H in pairs(A.selectedItems)do table.insert(D,E)end A.callback(D)else A.callback(A.selected)end end end function z.UpdateSelectedText(A)if A.multi then local B=0 for D in pairs(A.selectedItems)do B=B+1 end A.elements.selected.value.Text=B==0 and'None'or B..' selected'end end local A,B=game:GetService'UserInputService',{activePicker=nil,pickers={}}function B.Register(D,E)table.insert(D.pickers,E)end function B.SetActive(D,E)if D.activePicker and D.activePicker~=E then D.activePicker:ForceClose()end D.activePicker=E end function B.CloseAll(D)for E,H in ipairs(D.pickers)do H:ForceClose()end D.activePicker=nil end local D={}D.__index=D function D.new(E)local H=setmetatable({text=E.text,default=E.default or Color3.fromRGB(255,255,255),callback=E.callback,section=E.section,main=E.section.main,current=nil,opened=false,dragging=false,hueDragging=false,currentHue=0,animations={},connections={},menuWidth=200,elements={}},D)B:Register(H)H:Create()return H end function D.Create(E)local H,I=f.Instance('ScreenGui',{Name='ColorPickerPortal',Parent=game:GetService'Players'.LocalPlayer:WaitForChild'PlayerGui'}),f.Instance('Frame',{Name='ColorPicker',Size=UDim2.fromScale(1,0),AutomaticSize=Enum.AutomaticSize.Y,BackgroundTransparency=1,Parent=E.section.current.value})I:Layout{FillDirection=Enum.FillDirection.Vertical,Padding=UDim.new(0,8)}local J=f.Instance('Frame',{Size=UDim2.fromScale(1,0),AutomaticSize=Enum.AutomaticSize.Y,BackgroundTransparency=1,Parent=I.value})J:Layout{FillDirection=Enum.FillDirection.Horizontal,VerticalAlignment=Enum.VerticalAlignment.Center,Padding=UDim.new(0,8)}local K=f.createText{Text=E.text,TextColor3=E.main.theme.Colors.Text,TextSize=14,Parent=J.value}K:Flex{FlexMode=Enum.UIFlexMode.Fill}local L=f.Instance('TextButton',{Size=UDim2.fromOffset(30,30),BackgroundColor3=E.default,AutoButtonColor=false,Text='',Parent=J.value})L:Rounded(6)local M=f.Instance('Frame',{Name='MenuContainer',Size=UDim2.fromOffset(E.menuWidth,0),AutomaticSize=Enum.AutomaticSize.Y,BackgroundColor3=E.main.theme.Colors.Ground,Visible=false,Parent=game.Players.LocalPlayer.PlayerGui.RoyxGui})M:Rounded(12)M:Padding(12)M:Layout{FillDirection=Enum.FillDirect